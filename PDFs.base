filters:
  and:
    - file.ext == "pdf"
views:
  - type: table
    name: Table
    order:
      - file.name
      - file.folder
    sort:
      - property: file.folder
        direction: ASC
      - property: file.basename
        direction: DESC
      - property: file.path
        direction: ASC
    columnSize:
      file.name: 831
